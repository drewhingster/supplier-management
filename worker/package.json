{
  "name": "supplier-management-api",
  "version": "1.0.0",
  "description": "Cloudflare Worker API for Supplier Document Management System - Bureau of Statistics",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:init": "wrangler d1 execute supplier-management-db --file=./schema.sql",
    "db:init:local": "wrangler d1 execute supplier-management-db --local --file=./schema.sql",
    "tail": "wrangler tail"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "supplier-management",
    "procurement"
  ],
  "author": "Bureau of Statistics - Procurement Unit",
  "license": "UNLICENSED",
  "devDependencies": {
    "wrangler": "^3.0.0"
  }
}
